{
    "[python]": {
        "editor.formatOnSave": true
    },
    "csharpsortusings.sort.usings.splitGroups": false,
    "cSpell.userWords": [
        "reddit"
    ],
    "debug.onTaskErrors": "showErrors",
    "debug.toolBarLocation": "docked",
    "editor.bracketPairColorization.enabled": true,
    "editor.fontFamily": "'JetBrainsMono Nerd Font', 'FiraMono Nerd Font', 'VictorMono Nerd Font', 'CaskaydiaCove Nerd Font', 'Cascadia Code', 'Fira Code', Consolas, Monaco, Menlo, Monaco, 'Courier New', monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 11,
    "editor.inlineSuggest.enabled": true,
    "editor.linkedEditing": true,
    "editor.minimap.enabled": false,
    "editor.mouseWheelZoom": true,
    "editor.rulers": [
        80,
        100
    ],
    "editor.stickyScroll.enabled": true,
    "editor.suggestSelection": "first",
    "emmet.includeLanguages": {
        "django-html": "html"
    },
    "emmet.triggerExpansionOnTab": true,
    "files.exclude": {
        "**/.classpath": true,
        "**/.factorypath": true,
        "**/.project": true,
        "**/.settings": true
    },
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "git.mergeEditor": true,
    "github.copilot.enable": {
        "*": true,
        "markdown": false,
        "plaintext": false,
        "python": false,
        "yaml": false
    },
    "gitlens.currentLine.enabled": false,
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.keymap": "alternate",
    "gruvboxMaterial.darkContrast": "soft",
    "gruvboxMaterial.lightContrast": "soft",
    "hediet.vscode-drawio.local-storage": "",
    "macros": {
        "saveAndExitVimInsertMode": [
            "workbench.action.files.save",
            "extension.vim_escape"
        ]
    },
    "mssql.connections": [
        {
            "authenticationType": "Integrated",
            "connectionString": "Server=(localdb)\\localdev;Integrated Security=True;Initial Catalog=discogs",
            "database": "discogs",
            "password": "",
            "profileName": "LocalDev-Discogs",
            "server": "(localdb)\\localdev",
            "user": ""
        }
    ],
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "python.defaultInterpreterPath": "C:\\Python39\\python.exe",
    "python.formatting.blackArgs": [
        "--line-length",
        "120"
    ],
    "python.formatting.provider": "black",
    "python.jediEnabled": false,
    "python.languageServer": "Pylance",
    "python.linting.prospectorEnabled": true,
    "python.linting.pylintEnabled": false,
    "python.testing.pytestEnabled": true,
    "sqltools.connections": [
        {
            "mssqlOptions": {
                "appName": "SQLTools",
                "useUTC": true,
                "encrypt": true
            },
            "previewLimit": 50,
            "server": "localhost",
            "port": 1433,
            "driver": "MSSQL",
            "name": "E360 Local",
            "group": "InTech",
            "database": "energy360",
            "username": "SA",
            "password": "MyP4ssw0rd"
        }
    ],
    "sqltools.highlightQuery": false,
    "sqltools.autoOpenSessionFiles": false,
    "terminal.integrated.autoReplies": {
        "Terminate batch job (Y/N)": "Y\r"
    },
    "terminal.integrated.rendererType": "dom",
    "terminal.integrated.shellIntegration.enabled": true,
    "vim.easymotion": true,
    "vim.easymotionMarkerBackgroundColor": "#606060FF", // '#0000'
    "vim.easymotionMarkerForegroundColorOneChar": "#FEE715FF", // '#ff0000'
    "vim.easymotionMarkerForegroundColorTwoCharFirst": "#ADEFD1FF", // "#ffb400",
    "vim.easymotionMarkerForegroundColorTwoCharSecond": "#D6ED17FF", // "#b98300",
    "vim.foldfix": true,
    "vim.gdefault": true,
    "vim.insertModeKeyBindings": [
        {
            "after": [
                "<Esc>"
            ],
            "before": [
                "j",
                "j"
            ]
        },
        {
            "after": [],
            "before": [
                "<C-e>"
            ],
            "commands": [
                {
                    "command": "cursorEnd"
                }
            ]
        },
        {
            "after": [],
            "before": [
                "<C-a>"
            ],
            "commands": [
                {
                    "command": "cursorHome"
                }
            ]
        }
    ],
    "vim.useSystemClipboard": true,
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "window.autoDetectColorScheme": true,
    "window.commandCenter": true,
    "window.menuBarVisibility": "toggle",
    "window.restoreWindows": "none",
    "window.titleBarStyle": "custom",
    "window.zoomLevel": 1,
    "workbench.activityBar.visible": true,
    "workbench.colorTheme": "Dracula",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "workbench.preferredDarkColorTheme": "Dracula",
    "workbench.preferredLightColorTheme": "Solarized Light",
    "workbench.sideBar.location": "right",
    "workbench.tree.indent": 16,
    "workbench.welcomePage.hiddenCategories": [
        "Beginner",
        "Setup"
    ],
    "terminal.integrated.env.osx": {
        "FIG_NEW_SESSION": "1"
    },
    "editor.accessibilitySupport": "off",
    "files.association": {
        "*.vim": "vimrc",
        "vimrc": "vimrc",
        "gvimrc": "vimrc"
    },
    "diffEditor.experimental.collapseUnchangedRegions": true,
    "editor.fontWeight": "normal",
    "githubPullRequests.fileListLayout": "tree"
}